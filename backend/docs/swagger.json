{
    "swagger": "2.0",
    "info": {
        "description": "WsAI 项目 Swagger 文档",
        "title": "WsAI Backend API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:9091",
    "basePath": "/",
    "paths": {
        "/api/v1/AI/chatMessage/sessions": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "返回当前登录用户创建的所有聊天会话（通常按创建时间降序）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "会话管理"
                ],
                "summary": "获取当前用户的所有会话列表",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/internal_handler_session.GetUserSessionsResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/AI/chatMessage/sessions/stream": {
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "创建一个新会话，同时把用户的第一个问题发给 AI，使用 SSE 流式返回",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "text/event-stream"
                ],
                "tags": [
                    "会话管理"
                ],
                "summary": "创建新会话 + 发送第一个问题（SSE 流式返回）",
                "parameters": [
                    {
                        "description": "请求参数",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handler_session.CreateSessionAndSendFirstMessageRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "SSE error 事件",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/AI/chatMessage/sessions/{session_id}/messages": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "返回指定会话 ID 下的所有消息记录（按时间升序）",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "会话管理"
                ],
                "summary": "获取指定会话的全部历史消息",
                "parameters": [
                    {
                        "type": "string",
                        "description": "会话ID",
                        "name": "session_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/internal_handler_session.GetMeaasgeHistoryResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/AI/chatMessage/sessions/{session_id}/messages/stream": {
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "向指定的会话 ID 发送一条新消息，并使用 SSE 流式返回 AI 回答",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "text/event-stream"
                ],
                "tags": [
                    "会话管理"
                ],
                "summary": "向已有会话追加消息（SSE 流式返回）",
                "parameters": [
                    {
                        "type": "string",
                        "description": "会话ID",
                        "name": "session_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "请求参数",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handler_session.SendMessageStreamRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "SSE error 事件",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/v1/user/captcha": {
            "post": {
                "description": "向指定邮箱发送注册验证码",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "发送邮箱验证码",
                "parameters": [
                    {
                        "description": "邮箱参数",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.CaptchaRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "验证码发送成功",
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.CaptchaResponse"
                        }
                    },
                    "400": {
                        "description": "邮箱格式错误",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    },
                    "429": {
                        "description": "发送过于频繁",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    }
                }
            }
        },
        "/api/v1/user/login": {
            "post": {
                "description": "根据用户名和密码登录，返回 JWT token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "description": "登录参数",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功，返回 token",
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.LoginResponse"
                        }
                    },
                    "400": {
                        "description": "参数错误",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    },
                    "401": {
                        "description": "用户名或密码错误",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    }
                }
            }
        },
        "/api/v1/user/users": {
            "post": {
                "description": "通过邮箱、密码和验证码注册新用户，成功后直接返回登录 token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户认证"
                ],
                "summary": "用户注册",
                "parameters": [
                    {
                        "description": "注册参数",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.RegisterRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "注册成功，返回 token",
                        "schema": {
                            "$ref": "#/definitions/internal_handler_user.RegisterResponse"
                        }
                    },
                    "400": {
                        "description": "参数错误或验证码错误",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    },
                    "409": {
                        "description": "用户已存在",
                        "schema": {
                            "$ref": "#/definitions/wsai_backend_internal_common.Response"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "internal_handler_session.CreateSessionAndSendFirstMessageRequest": {
            "type": "object",
            "required": [
                "modelType",
                "question"
            ],
            "properties": {
                "modelType": {
                    "type": "string"
                },
                "question": {
                    "type": "string"
                }
            }
        },
        "internal_handler_session.GetMeaasgeHistoryResponse": {
            "type": "object",
            "properties": {
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                }
            }
        },
        "internal_handler_session.GetUserSessionsResponse": {
            "type": "object",
            "properties": {
                "sessions": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/wsai_backend_internal_model.SessionInfo"
                    }
                },
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                }
            }
        },
        "internal_handler_session.SendMessageStreamRequest": {
            "type": "object",
            "required": [
                "modelType",
                "question",
                "sessionId"
            ],
            "properties": {
                "modelType": {
                    "type": "string"
                },
                "question": {
                    "type": "string"
                },
                "sessionId": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.CaptchaRequest": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.CaptchaResponse": {
            "type": "object",
            "properties": {
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.LoginRequest": {
            "type": "object",
            "properties": {
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.LoginResponse": {
            "type": "object",
            "properties": {
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.RegisterRequest": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "captcha": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "internal_handler_user.RegisterResponse": {
            "type": "object",
            "properties": {
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "wsai_backend_internal_common.Response": {
            "type": "object",
            "properties": {
                "status_code": {
                    "$ref": "#/definitions/wsai_backend_internal_common_code.Code"
                },
                "status_msg": {
                    "type": "string"
                }
            }
        },
        "wsai_backend_internal_common_code.Code": {
            "type": "integer",
            "format": "int64",
            "enum": [
                1000,
                2001,
                2002,
                2003,
                2004,
                2005,
                2006,
                2007,
                2008,
                2009,
                2010,
                3001,
                4001,
                5001,
                5002,
                5003
            ],
            "x-enum-varnames": [
                "CodeSuccess",
                "CodeInvalidParams",
                "CodeUserExist",
                "CodeUserNotExist",
                "CodeInvalidPassword",
                "CodeNotMatchPassword",
                "CodeInvalidToken",
                "CodeNotLogin",
                "CodeInvalidCaptcha",
                "CodeRecordNotFound",
                "CodeIllegalPassword",
                "CodeForbidden",
                "CodeServerBusy",
                "AIModelNotFind",
                "AIModelCannotOpen",
                "AIModelFail"
            ]
        },
        "wsai_backend_internal_model.SessionInfo": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "sessionId": {
                    "type": "string"
                }
            }
        }
    }
}